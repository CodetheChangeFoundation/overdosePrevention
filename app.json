{
  "expo": {
    "name": "Overdose Prevention",
    "description": "This project is really great.",
    "slug": "overdosePrevention",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ca.codethechange.overdoesprevention",
      "config": {
        "googleMapsApiKey": "YOUR IOS GOOGLE MAP API KEY HERE"
      }
    },
    "android": {
      "package": "ca.codethechange.overdoesprevention",
      "config": {
        "googleMaps": { "apiKey": "YOUR ANDROID GOOGLE MAP API KEY HERE" }
      }
    }
  }
}
